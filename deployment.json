{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {  },
    "variables": {  },
    "functions": [  ],
    "resources": [ 
{
     "apiVersion": "2017-05-10",
     "name": "linkedTemplatevnet",
     "dependsOn": [],
     "type": "Microsoft.Resources/deployments",
     "properties": {
       "mode": "Incremental",
       "templateLink": {
          "uri":"https://raw.githubusercontent.com/xelex2007/azure-module-4/master/vnet.json",
          "contentVersion":"1.0.0.0"
       }
     }
  },
  {
   "apiVersion": "2017-05-10",
   "name": "linkedTemplatekeyvault",
   "dependsOn": [],
   "type": "Microsoft.Resources/deployments",
   "properties": {
     "mode": "Incremental",
     "templateLink": {
        "uri":"https://raw.githubusercontent.com/xelex2007/azure-module-4/master/key_vault.json",
        "contentVersion":"1.0.0.0"
     },
     "parameters" :{
      "adminname" : {
         "type" : "string"
         
     },
      "adminpassword" : {
         "type" : "securestring"
         
       }
     }
     
   }
}, {
   "apiVersion": "2017-05-10",
   "name": "linkedTemplatevmdeploy",
   "dependsOn": [
      "linkedTemplatekeyvault",
      "linkedTemplatevnet"
   ],
   "type": "Microsoft.Resources/deployments",
   "properties": {
     "mode": "Incremental",
     "templateLink": {
        "uri":"https://raw.githubusercontent.com/xelex2007/azure-module-4/master/vm_deploy.json",
        "contentVersion":"1.0.0.0"
     },
     "parameters" :{
         "publicipname": {
            "type": "string",
            "value": "[reference('linkedTemplatevnet').outputs.publicipname.value]"
            },
            "vnetname": {
               "type": "string",
               "value": "[reference('linkedTemplatevnet').outputs.vnetname.value]"
            },
            "adminname": {
               "type": "string",
               "value": "[reference('linkedTemplatekeyvault').outputs.nameofuser.value]"
            },
            "adminpass": {
               "type": "string",
               "value": "[reference('linkedTemplatekeyvault').outputs.passofuser.value]"
            }

     }
     
   }
}
	],
    "outputs": { 
      
     }
}